<Window x:Class="JointLessonTerminal.MVVM.View.WordDocumentPreviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModel="clr-namespace:JointLessonTerminal.MVVM.ViewModel"
        xmlns:local="clr-namespace:JointLessonTerminal.MVVM.View"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance viewModel:WordDocumentPreviewViewModel}"
        Title="WordDocumentPreviewWindow" Height="450" Width="800">
    <Grid>
        <DocumentViewer Grid.Row="0" Document="{Binding ActiveDocument}" />
    </Grid>
</Window>
